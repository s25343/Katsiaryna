#include<iostream>
using namespace std;

int main ()
{
  int a[10], n, i, c, t;
  cout << "Wpisz ilosc liczb: \n";
  cin >> n;
  cout << "Wypisz liczby: \n";
  for (i = 0; i < n; ++i)
    cin >> a[i];

  for (i = 1; i < n; ++i)
    {
      for (c = 0; c < (n - i); ++c)
	if (a[c] > a[c + 1])
	  {
	    t = a[c];
	    a[c] = a[c + 1];
	    a[c + 1] = t;
	  }
    }

  cout << "Posortowane licziy:" << ' ';
  for (i = 0; i < n; ++i)
    cout << ' ' << a[i];

  return 0;

}
