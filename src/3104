#include <iostream>

using namespace std;

int main()
{
    
    int a[10] = { 96, 63, 24, 67, 37, 9, 43, 58, 24, 6 };
    int min = a[0];
    int ind;
    
    for(int i=0; i<10; i++){
        cout << a[i] << ' ';
        
      }
      cout << endl;
    
    
      for(int i=0; i<10; i++){
        if(min>a[i])
            ind = i;
      }
      cout << "index min = " << ind << endl;
    return 0;
}
